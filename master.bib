@article{richards_kritzer2002,
     jstor_articletype = {research-article},
     title = {Jurisprudential Regimes in Supreme Court Decision Making},
     author = {Richards, Mark J. and Kritzer, Herbert M.},
     journal = {American Political Science Review},
     jstor_issuetitle = {},
     volume = {96},
     number = {2},
     jstor_formatteddate = {Jun., 2002},
     pages = {305-320},
     url = {http://www.jstor.org/stable/3118027},
     ISSN = {00030554},
     abstract = {We theorize that if law matters in Supreme Court decision making, it matters not as a mechanistic force that dictates decisions, but as an institutional construct created by justices who possess political attitudes. Jurisprudential regimes identify relevant case factors and/or set the level of scrutiny or balancing the justices will use. These jurisprudential regimes have the potential to make a significant difference in the decisions of the justices. We identify a candidate jurisprudential regime, content-neutrality, which appears to govern the general area of free expression law. The Court applies the strictest standard of review to regulations of expression that target the content or viewpoint of expression. Relying on a series of statistical tests using logistic regression, we find that the justices take seriously this jurisprudential regime.},
     language = {English},
     year = {2002},
     publisher = {American Political Science Association},
     copyright = {Copyright © 2002 American Political Science Association},
    }

    @article{kritzards2003,
